pipeline {
    agent any
    options {
        skipStagesAfterUnstable()
    }
    stages {
        stage('Download') {
            steps {
                sh 'git clone git@github.com:albertoalejandrolariosmanzano/challenge2.git'
            }
        }
        stage('Running') {
            steps {
                sh 'cd challenge2 && docker-compose up -d --build'
            }
        }
    }
}
